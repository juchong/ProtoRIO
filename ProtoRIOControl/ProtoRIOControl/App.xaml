<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProtoRIOControl.App">
	<Application.Resources>
        <ResourceDictionary>
            <Color x:Key="batteryHighColor">#008000</Color>
            <Color x:Key="batteryMedColor">#ffff00</Color>
            <Color x:Key="batteryLowColor">#ff0000</Color>
            <Color x:Key="redLabelColor">#ff0000</Color>
            <Color x:Key="greenLabelColor">#008000</Color>
            <Color x:Key="onColor">#008000</Color>
            <Color x:Key="offColor">#ff0000</Color>
            <Color x:Key="grayButtonColor">#656565</Color>
